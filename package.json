{
  "name": "yaal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": "0xWelt",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xWelt/yaal"
  },
  "bugs": {
    "url": "https://github.com/0xWelt/yaal/issues"
  },
  "homepage": "https://0xWelt.github.io/yaal/",
  "scripts": {
    "dev": "node scripts/generate-github-config.js && node scripts/parse-readme.js && next dev",
    "build": "node scripts/generate-github-config.js && node scripts/parse-readme.js && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "parse-readme": "node scripts/parse-readme.js",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,yml,yaml}": [
      "prettier --write"
    ]
  }
}
